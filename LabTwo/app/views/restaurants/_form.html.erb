<%= form_for :restaurant, :html => {:multipart => true}, url: restaurants_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :phone_number %><br>
    <%= f.text_area :phone_number %>
  </p>

  <p>
    <%= f.label :cuisine %><br>
    <%= f.text_area :cuisine %>
  </p>

  <p>
    <%= f.label :address %><br>
    <%= f.text_field :address %>
  </p>
 
  <p>
    <%= f.file_field :image %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
